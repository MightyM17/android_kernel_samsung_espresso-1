			0x374	0x00040009 /* vin5a_clk0 OUTPUT | MODE0 */
			0x39c	0x00040009 /* vin5a_hsync0 OUTPUT | MODE0 */
			0x3a0	0x00040009 /* vin5a_vsync0 OUTPUT | MODE0 */
			0x398	0x00040009 /* vin5a_d0	OUTPUT | MODE0 */
